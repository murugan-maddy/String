import java.util.*;
import java.lang.*;
class Main
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
String a=sc.nextLine();
String b[]=a.split(" ");
String First_Letter_Caps="";
for(int i=0;i<b.length;i++)
{
String first_letter=b[i].substring(0,1);
String remaining_letter=b[i].substring(1);
First_Letter_Caps+=first_letter.toUpperCase()+remaining_letter+" ";
}
System.out.print(First_Letter_Caps);
}
}
